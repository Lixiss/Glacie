<partial>
  <record-type name="InGameUI.EnchanterReagentBox" path="InGameUI/EnchanterReagentBox.tpl">
    <inherits>
      <inherit record="InGameUI.EnchanterItemBox" />
    </inherits>
    <var-group>
      <var-group name="Header" system="true">
        <field name="ActorName" var-class="variable" var-type="string" />
        <field name="Class" var-class="static" var-type="string" />
        <field name="FileDescription" var-class="variable" var-type="string" />
      </var-group>
      <var-group name="Rollover Config">
        <field name="rolloverTagNoFormula" var-class="variable" var-type="string">
          <description>No Formula Message</description>
        </field>
        <field name="rolloverTagNotAvailable" var-class="variable" var-type="string">
          <description>Error Message</description>
        </field>
        <field name="textStyleNameDisplay" var-class="variable" var-type="file_dbr">
          <description>Reagent Name</description>
        </field>
        <field name="textStyleNameNoAvailable" var-class="variable" var-type="file_dbr">
          <description>Error Message</description>
        </field>
        <field name="textStyleNoFormula" var-class="variable" var-type="file_dbr">
          <description>No Formula Style</description>
        </field>
      </var-group>
    </var-group>
  </record-type>
</partial>