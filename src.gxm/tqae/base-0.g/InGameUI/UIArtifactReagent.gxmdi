<partial>
  <record-type name="InGameUI.UIArtifactReagent" path="InGameUI/UIArtifactReagent.tpl">
    <inherits>
      <inherit record="InGameUI.Includes.StringRollover" />
    </inherits>
    <var-group>
      <var-group name="Header" system="true">
        <field name="ActorName" var-class="variable" var-type="string" />
        <field name="Class" var-class="static" var-type="string" />
        <field name="FileDescription" var-class="variable" var-type="string" />
      </var-group>
      <var-group name="Config">
        <field name="itemX" var-class="variable" var-type="int" />
        <field name="itemXSize" var-class="variable" var-type="int" />
        <field name="itemY" var-class="variable" var-type="int" />
        <field name="itemYSize" var-class="variable" var-type="int" />
      </var-group>
      <var-group name="Rollover Config">
        <field name="rolloverTagAvailableCount" var-class="variable" var-type="string" />
        <field name="rolloverTagNotAvailable" var-class="variable" var-type="string" />
        <field name="textStyleNameAvailable" var-class="variable" var-type="file_dbr" />
        <field name="textStyleNameNotAvailable" var-class="variable" var-type="file_dbr" />
      </var-group>
    </var-group>
  </record-type>
</partial>