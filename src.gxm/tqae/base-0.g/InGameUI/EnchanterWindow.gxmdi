        <field name="enchanterNameText" var-class="variable" var-type="file_dbr" />
        <field name="enchanterRecoverTab" var-class="variable" var-type="file_dbr" />
        <field name="enchanterRecoverTabBitmap" var-class="variable" var-type="file_dbr" />
