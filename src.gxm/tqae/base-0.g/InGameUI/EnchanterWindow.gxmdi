        <field name="enchanterRecoverTabButtonRollover" var-class="variable" var-type="file_dbr" />
        <field name="enchanterWindowTitle" var-class="variable" var-type="file_dbr" />
      </var-group>
    </var-group>
  </record-type>
</partial>